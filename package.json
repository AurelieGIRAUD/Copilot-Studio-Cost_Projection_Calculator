{
  "name": "copilot-studio-cost-calculator",
  "version": "1.0.0",
  "description": "Interactive 2-year cost projection calculator for Microsoft Copilot Studio",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
